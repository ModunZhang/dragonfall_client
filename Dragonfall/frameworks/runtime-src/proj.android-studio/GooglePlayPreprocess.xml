<?xml version="1.0" encoding="UTF-8"?>
<project name="GooglePlayPreprocess" default="GooglePlayPreprocess">
	<property name="wtk.home" value="tools"/>
	<taskdef resource="antenna.properties" classpath="tools/antenna-bin-1.0.2.jar"/>
    <target name="GooglePlayPreprocess" description="preprocess java source code with marco in GooglePlayPreprocess.txt">
       <wtkpreprocess
			verbose="true"
			srcdir="app/src"
			destdir="app/src"
			printsymbols="true"
			debuglevel="debug"
			encoding="UTF-8">
			<symbols_file name="GooglePlayPreprocess.txt"/>
		</wtkpreprocess>
    </target>
</project>

