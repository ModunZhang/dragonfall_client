<?xml version="1.0" encoding="UTF-8"?>
<project name="PayPalPreprocess" default="PayPalPreprocess">
	<property name="wtk.home" value="tools"/>
	<taskdef resource="antenna.properties" classpath="tools/antenna-bin-1.0.2.jar"/>
    <target name="PayPalPreprocess" description="preprocess java source code with marco in PayPalPreprocess.txt">
       <wtkpreprocess
			verbose="true"
			srcdir="app/src"
			destdir="app/src"
			printsymbols="true"
			debuglevel="debug"
			encoding="UTF-8">
			<symbols_file name="PayPalPreprocess.txt"/>
		</wtkpreprocess>
    </target>
</project>